page {
  background-color: #fff;
}
.body {
  margin: 0;
  padding: 0;
  background-color: #f6f8fe;
}
.nav {
  position: fixed;
  top: 0;
  left: 0;
  /* margin-bottom: 20rpx; */
  width: 648rpx;
  height: 60rpx;
  background-color: #fff;
  font-size: 26rpx;
  display: flex;
  flex-direction: row;
  border-top: 1px solid #cdcdcd;
  /* border-bottom: 1px solid #cdcdcd; */
  z-index: 2;
  padding: 30rpx 51rpx 40rpx 51rpx;
}
.nav > .active {
  line-height: 60rpx;
  text-align: center;
  flex: 1;
  background: #001a88;
  color: #fff;
}
.nav > .default {
  line-height: 60rpx;
  text-align: center;
  flex: 1;
  color: #333;
  border: solid 1px #e8e8e8;
}
.show {
  display: block;
  background-color: #fff;
}
.hidden {
  display: none;
  background-color: #f5f5f5;
}
#noMoney {
  margin-top: 150rpx;
  width: 750rpx;
  height: 100%;
}
#noMoney > .oderInfo {
  margin-bottom: 20rpx;
  background-color: #fff;
  position: relative;
  padding: 0 50rpx;
}
.oderInfo::after{
  content: "";
  height: 20rpx;
  width: 100%;
  background: #f6f8fe;
  position: absolute;
  left: 0;
  top: 0;
}
#noMoney > .oderInfo > .header {
  /* padding: 0 30rpx; */
  height: 90rpx;
  line-height: 90rpx;
  font-size: 26rpx;
  color: #333;
  border-bottom: 1px solid #e8e8e8;
  padding-top:20rpx;
}
#noMoney > .oderInfo > .header > text {
  float: right;
  color: #001a88;
}
#noMoney > .oderInfo > .body {
  margin: 30rpx 0;
  background-color: #fff;
}
#noMoney > .oderInfo > .body > .shopList {
  padding: 30rpx 0;
  overflow: hidden;
  border-bottom: 1px dashed #e8e8e8;
}
#noMoney > .oderInfo > .body > .shopList:first-child {
  padding-top: 0;
}
#noMoney > .oderInfo > .body > .shopList:last-child {
  border-bottom: none;
}
#noMoney > .oderInfo > .body > .shopList > image {
  float: left;
  width: 180rpx;
  height: 180rpx;
  background-color: #f5f5f5;
}
#noMoney > .oderInfo > .body > .shopList > .shopInfo {
  float: left;
  position: relative;
  margin-left: 30rpx;
  width: 430rpx;
  height: 180rpx;
}
#noMoney > .oderInfo > .body > .shopList > .shopInfo > .titleLittle {
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
}
#noMoney > .oderInfo > .body > .shopList > .shopInfo > .titleLittle > .shopNum {
  float: right;
  font-size: 28rpx;
  color: #666;
}
#noMoney > .oderInfo > .body > .shopList > .shopInfo > .bodyLittle {
  font-size: 24rpx;
  color: #333;
  padding-top: 10rpx;
}
#noMoney > .oderInfo > .body > .shopList > .shopInfo > .kezi {
  font-size: 26rpx;
  color: #666;
  float:left;
  margin-left:14rpx;
}
#noMoney > .oderInfo > .body > .shopList > .shopInfo > .money {
  position: absolute;
  font-size: 26rpx;
  color: #333;
  bottom: 0;
  left: 0;
}
#noMoney > .oderInfo > .shoplastInfo {
  padding-right: 20rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 24rpx;
  color: #333;
  text-align: right;
  border-top: 1px solid #e8e8e8;
  /* border-bottom: 1px solid #e8e8e8; */
}
#noMoney > .oderInfo > .shoplastInfo text{
  color: #FF0000;
}
#noMoney > .oderInfo > .shopButtom {
  /* padding: 29rpx 0rpx; */
  text-align: right;
  font-size: 30rpx;
  color: #666;
}
#noMoney > .oderInfo > .shopButtom:after {
  content: ".";
  clear: both;
  display: block;
  height: 0;
  overflow: hidden;
  visibility: hidden;
}
#noMoney > .oderInfo > .shopButtom > .inbom {
   margin-left: 20rpx; 
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #333;
  text-align: center;
  border-radius: 4rpx;
  border: 1px solid #BDBDBD;
  display: inline-block;
}
#noMoney > .oderInfo > .shopButtom > .justQX{
  float:left;
  line-height: 60rpx;
  margin-left: 40rpx;
  font-size: 26rpx;
}
#noMoney > .oderInfo > .shopButtom > .inbom:first-child {
  margin-left: 25rpx;
}
#noMoney > .oderInfo > .shopButtom > .zhifu {
  background-color: #001a88;
  color: #fff;
  border:none;
}
#noMoney > .noOrder {
  padding-top: 100rpx;
  height: 100%;
  text-align: center;
}
#noMoney > .noOrder > image {
  width: 200rpx;
  height: 200rpx;
}
#noMoney > .noOrder > .txt {
  padding-top: 30rpx;
  font-size: 26rpx;
  color: #333;
}





