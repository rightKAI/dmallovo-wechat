page {
  height: 100%;
  background-color: #f5f5f5;
}
.main {
  margin: 20rpx;
}
.main > .helpList {
  margin-bottom: 20rpx;
}
.main > .helpList .infull {
  padding: 0 30rpx 0 20rpx;
  height: 100rpx;
  background-color: #fff;
}
.main > .helpList .infull > text {
  line-height: 100rpx;
  font-size: 30rpx;
  color: #333;
}
.main > .helpList .infull > image {
  float: right;
  margin: 44rpx 0 0 0;
  width: 16rpx;
  height: 30rpx;
}
.main > .helpList .donwn {
  background-color: #fff;
}
.main > .helpList .donwn > .userTxt {
  margin: 0 30rpx;
  border-top: solid 1px #e8e8e8;
  height: 100rpx;
  line-height: 100rpx;
  color: #666;
  font-size: 30rpx;
}
.main > .helpList .nonoIn {
  display: none;
}
