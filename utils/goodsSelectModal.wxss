    line-height: 70rpx;
    background-color: #f5f5f5;
    font-size: 28rpx;
    color: #333;
    border-radius: 4px;
    text-align: center;
    margin: 0 40rpx 27rpx 0;
    border:1px solid #bdbdbd;
}
.selectModal .Collocation .shopClass .notClick{
  border: 1px dashed #cdcdcd;
}
.selectModal .Collocation .shopClass .banClass{
  background: #f6f8fe;
  color: #cdcdcd;
  border: 1px dashed #cdcdcd;
}
.selectModal .Collocation .shopClass text:nth-child(3n){
    margin-right: 0;
}
