}
.pop_box > .pop_con > .popborder > .pop_text > .btn {
  margin-top: 25rpx;
}
.pop_box > .pop_con > .popborder > .pop_text > .btn > text {
  display: inline-block;
  width: 135rpx;
  height: 62rpx;
