#fixMenu{
  position: fixed;
  bottom:0;
  left:0;
  width: 100%;
  height:100rpx;
  background: #fff;
  z-index: 300;
  border-top: 1rpx solid #cdcdcd;
}
#fixMenu:after{
    content:".";
    clear:both;
    display:block;
    height:0;
    overflow:hidden;
    visibility:hidden;
}
#fixMenu .item{
  float: left;
  padding-top: 15rpx;
  margin-right: 0;
  color: #666;
}
#fixMenu .items{
  color: #001a88;
}
#fixMenu .item:last-child{
  /* margin-left: 45rpx;
  margin-right: 45rpx; */
  padding-left:55rpx;
  /* padding-right:55rpx; */
}
#fixMenu .item:first-child{
  /* margin-left: 42rpx;
  margin-right: 45rpx; */
  padding-left:52rpx;
  padding-right:55rpx;
}
#fixMenu .item:nth-child(2){
   /* margin-left: 45rpx;
  margin-right: 45rpx; */
  padding-left:55rpx;
  padding-right:55rpx;
}
#fixMenu .item:nth-child(3){
   /* margin-left: 45rpx;
  margin-right: 45rpx; */
  padding-left:55rpx;
  padding-right:55rpx;
}
#fixMenu .item:nth-child(4){
   /* margin-left: 45rpx;
  margin-right: 45rpx; */
  padding-left:55rpx;
  padding-right:45rpx;
}
#fixMenu .item text:nth-child(1){
  display: block;
  margin-bottom: 10rpx;
}
#fixMenu .item text:nth-child(2){
  display: block;
  font-size: 20rpx;
}
