page {
  width:100%;
  max-width:100%;
  -webkit-user-select:none;
  user-select:none;
  background: #fff;
  border-top: 1px solid #cdcdcd;
  overflow-x:hidden;
}

#app {
  width:100%;
  overflow-x:hidden;
  padding-bottom: 66rpx;
  background: #f6f8fe;
}
.clearBoth:after{
    content:".";
    clear:both;
    display:block;
    height:0;
    overflow:hidden;
    visibility:hidden;
}
.content .info{
  padding: 40rpx 26rpx 42rpx 26rpx;
  color:#333;
  font-size: 30rpx;
  border-bottom: 15rpx solid #f5f5f5;
}
.content .info .infoFirst{
  margin-bottom: 20rpx;
}
.content .logisticsFollow .title{
  color: #333;
  border-bottom: 1rpx solid #e8e8e8;
  padding: 30rpx 26rpx 32rpx 26rpx;
  font-size: 30rpx;
}
.content .logisticsFollow .detail{
  padding: 0 26rpx 0 52rpx;
}
.content .logisticsFollow .detail .child{
  padding: 30rpx 18rpx 0rpx 0;
  position: relative;
  color: #999;
  font-size: 26rpx;
  border-left: 4rpx solid #dddddd;
}
.content .logisticsFollow .detail .child:first-child{
  padding: 0rpx 18rpx 0rpx 0;
  color: #001a88;
}
.content .logisticsFollow .detail .child .detailInfo{
  margin-left: 54rpx;
  border-bottom: 1rpx solid #e8e8e8;
  padding-bottom: 30rpx;
}
.content .logisticsFollow .detail .child:last-child{
  border:none;
}
.content .logisticsFollow .detail .child .circular{
  width: 16rpx;
  height:16rpx;
  background: #dddddd;
  border-radius: 50%;
  position: absolute;
  left: -8rpx;
  top: 32rpx;
}
.content .logisticsFollow .detail .child .first-circular{
  background: #001a88;
  border-radius: 50%;
  width: 18rpx;
  height:18rpx;
  position: absolute;
  top:0rpx;
  left: -16rpx;
  border:7rpx solid #7f8cc3;
}
.content .logisticsFollow .detail .child .last-circular{
  width: 16rpx;
  height:16rpx;
  background: #dddddd;
  border-radius: 50%;
  position: absolute;
  left: -4rpx;
  top: 32rpx;
}
.content .logisticsFollow .detail .child .detailInfo view:first-child{
  margin-bottom: 16rpx;
}
.last-line{
  width: 4rpx;
  height: 32rpx;
  background: #dddddd;
  position: absolute;
  top: 0;
  left:0;
}


.order_header{
  width: 750rpx;
  height: 270rpx;
  background: url("https://img-dmallovo.oss-cn-shenzhen.aliyuncs.com/icon/ICON-WebChar/me/img_dingdanzhuangtai.png") no-repeat;
  background-size: cover;
  padding: 0 50rpx;
}
.order_await{
  font-size:42rpx;
  color: #fff;
  padding-top: 54rpx;
  padding-bottom: 40rpx;
}
.order_tem{
  font-size: 24rpx;
  color: #fff;
}
.order_time{
  padding-top: 16rpx;
}
.content{
  background: #f6f8fe;
}
.logisticsFollow{
  padding-top: 63rpx;
}