/*弹窗*/
.pop_box > .pop_bg {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
  z-index: 9999999;
  background: rgba(0, 0, 0, 0.5);
}
.pop_box > .pop_con {
  background: #1a2b7a;
  width: 650rpx;
  height: 900rpx;
  position: fixed;
  left: 50%;
  top: 50%;
  z-index: 10000000;
  margin-left: -325rpx;
  margin-top: -500rpx;
  color: #fff;
}
.pop_box > .pop_con > .popborder {
  width: 606rpx;
  height: 851rpx;
  background: #fff;
  margin: 25rpx 22rpx 24rpx;
}
.pop_box > .pop_con > .popborder > .pop_text {
  background: url("https://img-dmallovo.oss-cn-shenzhen.aliyuncs.com/icon/ICON-M/pop/bor.png") no-repeat;
  background-size: cover;
  width: 540rpx;
  height: 786rpx;
  text-align: center;
  position: relative;
  top: 32rpx;
  margin: 0 auto;
}
.pop_box > .pop_con > .popborder > .pop_text > .closeBtn {
  position: absolute;
  top: -70rpx;
  right: -70rpx;
  width: 60rpx;
  height: 60rpx;
}
.pop_box > .pop_con > .popborder > .pop_text > view {
  text-align: center;
}
.pop_box > .pop_con > .popborder > .pop_text > .logo {
  padding-top: 64rpx;
}
.pop_box > .pop_con > .popborder > .pop_text > .logo image {
  width: 203rpx;
  height: 106rpx;
}
.pop_box > .pop_con > .popborder > .pop_text > .title {
  padding-top: 50rpx;
  position: relative;
  z-index: 3;
}
.pop_box > .pop_con > .popborder > .pop_text > .title image {
  width: 335rpx;
  height: 91rpx;
}
.pop_box > .pop_con > .popborder > .pop_text > .shop {
  position: absolute;
  top: 300rpx;
  left: 165rpx;
  z-index: 2;
}
.pop_box > .pop_con > .popborder > .pop_text > .shop  {
  display: block;
}
.pop_box > .pop_con > .popborder > .pop_text > .shop > image {
  width: 218rpx;
  height: 218rpx;
}
.pop_box > .pop_con > .popborder > .pop_text > .cs {
  margin-top: 120rpx;
  position: relative;
  z-index: 3;
}
.pop_box > .pop_con > .popborder > .pop_text > .cs image {
  width: 48rpx;
  height: 4rpx;
}
.pop_box > .pop_con > .popborder > .pop_text > .tex {
  margin-top: 20rpx;
  position: relative;
  z-index: 3;
  display: inline-block;
  width: 300rpx;
