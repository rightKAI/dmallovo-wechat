/***************弹层*****************/
.selectModal{
  position: fixed;
  top: 0;
  width: 100%;
  background: rgba(0,0,0,0.8);
  z-index:10000;
  padding-top: 206rpx;
  height: 100%;
}
.selectModal .modalContent{
  width: 100%;
  background: #fff;
  height:100%;
  position: relative;
  padding-top: 105rpx;
  padding-bottom: 167rpx;
  z-index: 2000;
}
.selectModal .goodsTittle{
  position: fixed;
  top: 206rpx;
  width: 100%;
  background: #fff;
}
.selectModal .modalContent .mcItem:after{
    content:".";
    clear:both;
    display:block;
    height:0;
    overflow:hidden;
    visibility:hidden;
}
.selectModal .modalContent .modalInfo{
  overflow-y:auto;
  height: 100%;
}
.selectModal .modalContent .mcItem{
  margin: 0 50rpx;
}
.selectModal .modalContent .mcItem:first-child{
  border-bottom: 1px solid #e8e8e8;
}
.selectModal .modalContent .mcItem .closeBtn{
  float: right;
  padding:40rpx 50rpx;
}
.selectModal .modalContent .mcItem .closeBtn .mcclose{
  display: block;
  width: 26rpx;
  height: 26rpx;
  background:url(http://img-dmallovo.oss-cn-shenzhen.aliyuncs.com/icon/ICON-WebChar/parameter_clost.png) no-repeat center center;
  background-size:100% auto;
}
.selectModal .modalContent .mcItem .mciImg,.mciInfo{
  float: left;
}
.selectModal .modalContent .mcItem .mciImg image{
  width: 150rpx;
  height: 150rpx;
  margin-right: 19rpx;
  background:#f6f8fe;
}
.selectModal .modalContent .goodsCon{
  padding: 40rpx 0;
  border-bottom: 1px solid #e8e8e8;
}
.selectModal .modalContent .goodsCon .mciInfo .mciiName{
  font-size: 32rpx;
  color:#333;
  font-weight: bold;
  margin-top:-7rpx;
}
.selectModal .modalContent .goodsCon .mciInfo .mcii4C{
  font-size: 24rpx;
  color:#333;
  margin-top: 24rpx;
}
.selectModal .modalContent .goodsCon .mciInfo .mcii4C > text{
  margin-right: 10rpx;
}
.selectModal .modalContent .goodsCon .mciInfo .mcii4C > text:last-child{
  margin-right: 0rpx;
}
.selectModal .modalContent .goodsCon .mciInfo .mciiPrice{
  font-size: 30rpx;
  color:#001a88;
  margin-top: 25rpx;
}
.bold{
  font-weight: bold;
}
.selectModal  .Collocation{
  margin: 0 50rpx;
  padding-top: 40rpx;
}
.selectModal .Collocation .tittle{
  display: block;
