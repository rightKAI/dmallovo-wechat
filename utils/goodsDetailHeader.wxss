#app .header{
    position: fixed;
    width: 100%;
    height: 80rpx;
    top: 0;
    left: 0;
    text-align: center;
    background-color: #fff;
    border-bottom: 1px solid #e8e8e8;
    z-index: 99;
}
#app .header .tab{
  width: 294rpx;
  margin: 0 auto;
  height:100%;
}
#app .header .tab text:nth-child(2){
  margin: 0 50rpx;
}
#app .header .tab text:nth-child(1){
  margin-left:15rpx;
}
#app .header .tab text{
  line-height: 76rpx;
  font-size: 30rpx;
  display:block;
  float: left;
}
#app .header .tab .active{
  border-bottom: 2px solid #001a88;
  margin-top:2rpx;
}
.clearBoth:after{
    content:".";
    clear:both;
    display:block;
    height:0;
    overflow:hidden;
    visibility:hidden;
}