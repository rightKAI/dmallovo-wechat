}
.pop_box > .pop_con > .popborder > .pop_text > .btn > text:first-child {
  margin-right: 34rpx;
}


.two_pop_text {
  position: absolute;
  left: 80rpx;
  top: 80rpx;
  width: 490rpx;
  height: 742rpx;
  background: rgba(8, 26, 106, 0.9);
  z-index: 66;
  color: #fff;
  text-align: center;
}
.two_pop_text .tipH {
  font-weight: normal;
  font-size: 42rpx;
  padding-top: 200rpx;
  padding-bottom: 40rpx;
  position: relative;
}
.two_pop_text .tipH:after,
.two_pop_text .tipH:before {
      content: "";
      width: 50rpx;
      height: 2rpx;
      position: absolute;
      background: #fff;
      top: 230rpx;
    }
.two_pop_text .tipH:after {
      left: 80rpx;
    }
.two_pop_text .tipH:before {
      right: 80rpx;
    }
.two_pop_text .tipP {
  font-size: 28rpx;
  padding-bottom: 80rpx;
}
.two_pop_text .two_back {
  width: 308rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 6rpx;
  background: #fff;
  text-align: center;
  color: #001a88;
  display: inline-block;
  font-size: 28rpx;
}
.two_pop_text .two_close {
  font-size: 28rpx;
  padding-top: 30rpx;
}
